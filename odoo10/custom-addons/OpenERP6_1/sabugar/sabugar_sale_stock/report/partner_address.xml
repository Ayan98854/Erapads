<?xml version="1.0" encoding="ISO-8859-1"?>
<addresses>
    <address_id type="fields" name="id,name,origin,sale_id">
        <origin type="field" name="origin"/>
        <name type="field" name="name"/>
	<!--added by JD -->
	<remark type="field" name="rm"/>
	<phone type="field" name="sale_id.phone" />
	<mobile type="field" name="sale_id.mobile"  />
        <total type="field" name="sale_id.amount_total"/>
         <contact type="zoom" name="address_id">
            <partner_name type="field" name="partner_id.name"/>
            <name type="field" name="name"/>
            <street type="field" name="street"/>
            <street2 type="field" name="street2"/>
            <area type="field" name="area_id.name"/>
            <zip type="field" name="zip"/>
            <city type="field" name="city_id.name"/>
            <state type="field" name="state_id.name"/>
            <country type="field" name="country_id.name"/>
         </contact>

         <movelines type="zoom" name='move_lines'>
            <references type="field" name="product_id.default_code"/>
            <qty type="field" name="product_qty"/>
         </movelines>

    </address_id>

</addresses>
